{
  "ci": {
    "collect": {
      "staticDistDir": "./dist",
      "numberOfRuns": 2,
      "settings": {
        "preset": "desktop"
      },
      "url": [
        "/",
        "/audit"
      ]
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "warn",
          {
            "minScore": 0.9
          }
        ],
        "categories:seo": [
          "warn",
          {
            "minScore": 0.9
          }
        ]
      }
    }
  }
}
